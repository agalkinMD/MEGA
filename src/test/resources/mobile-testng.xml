<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="Мобильные клиенты" verbose="1" parallel="tests" thread-count="1">
    <test name="iPhone 6-Anton">
        <parameter name="deviceName" value="Test iOS (iPhone 6S Plus)"/>
        <parameter name="platform" value="iOS 11.2.5"/>
        <parameter name="udid" value="8d4edc432a8e839c9437db9cc597ef82f785b9e3"/>
        <parameter name="mobilePort" value="8100"/>
        <parameter name="serverPort" value="4725"/>

        <groups>
            <define name="all">
                <include name="check-in"/>
                <include name="functional"/>
            </define>
            <run>
                <include name="check-in"/>
            </run>
        </groups>

        <packages>
            <package name="ru.mobiledimension.megaapp.tests.mallselection"/>
            <package name="ru.mobiledimension.megaapp.tests.profile"/>
        </packages>
    </test>
</suite>
